{{ 'single-course.css' | asset_url | stylesheet_tag }}

<section class="nxt-singleCourse__area">
  <div class="nxt-container">
    <nav class="sidebar-nav">
      <ul class="sidebar-nav-list">
        <ul class="sidebar-nav-list">
          <li><a href="#summary" class="sidebar-nav-item sidebar-nav-item--active">Summary</a></li>
          <li><a href="#overview" class="sidebar-nav-item">Overview</a></li>
          <li><a href="#curriculum" class="sidebar-nav-item">Curriculum</a></li>
          <li><a href="#course-media" class="sidebar-nav-item">Course Media</a></li>
          <li><a href="#review" class="sidebar-nav-item">Review</a></li>
        </ul>
      </ul>
    </nav>
    <div class="nxt-singleCourse__content">
      <div id="summary" class="course-hero">
        <div class="course-hero__header">
          <nav class="course-hero__breadcrumbs" aria-label="Breadcrumb">
            <a href="/">Home</a>
            <span>/</span>
            <a href="/collections/all">Courses</a>
            <span>/</span>
            <span>{{  product.category }}</span>
          </nav>
          <h1 class="course-hero__title">
            {{ product.title }}
          </h1>
          <p class="course-hero__subtitle">
            {{ product.metafields.custom.course_short_description }}
          </p>
        </div>
        <div class="course-hero__media-row">
          <div class="course-hero__image-wrapper">
            <img
              class="course-hero__image"
              src="{{ product.featured_image | img_url: '752x565' }}"
              alt="{{ product.title }}"
            >
            {% if product.metafields.custom.course_media_url %}
              <a href="{{ product.metafields.custom.course_media_url }}" class="course-hero__play-btn" target="_blank">
                <!-- Play SVG here -->
                <svg width="68" height="68" viewBox="0 0 68 68" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect x="1" y="1" width="66" height="66" rx="33" fill="white" />
                  <rect x="1" y="1" width="66" height="66" rx="33" stroke="#F5F5F5" stroke-width="2" />
                  <rect width="24" height="24" transform="translate(25 22)" fill="white" />
                  <path d="M29.6236 25.3513C29.8548 25.2171 30.14 25.2162 30.3721 25.3488L44.3721 33.3488C44.6058 33.4824 44.75 33.7309 44.75 34C44.75 34.2691 44.6058 34.5177 44.3721 34.6512L30.3721 42.6512C30.14 42.7838 29.8548 42.7829 29.6236 42.6487C29.3923 42.5145 29.25 42.2674 29.25 42V26C29.25 25.7326 29.3923 25.4855 29.6236 25.3513Z" fill="black" />
                </svg>
              </a>
            {% endif %}
          </div>
          <div class="nxt-module__overview">
            <!-- Add dynamic overview content if needed -->
          </div>
        </div>
      </div>
      <div class="course-details__wrapper">
        <div class="course-details__main">
          <!-- Overview Section -->
          <div id="overview" class="course-details__section course-details__overview">
            <h2 class="course-details__title">Overview</h2>
            <div class="course-details__content">
              {{ product.description }}
            </div>
          </div>

          <!-- Curriculum Section -->
