          <!-- Course Media Section -->
          <div id="course-media" class="course-details__section course-details__course-media">
            <h2 class="course-details__title">Course Media</h2>
            <div class="course-media-row">
              <div class="course-media__image-wrapper">
                <img
                  class="course-hero__image"
                  src="{{ product.featured_image | img_url: '752x565' }}"
                  alt="{{ product.title }}"
                >
                {% if product.metafields.custom.course_media_url %}
                  <a href="{{ product.metafields.custom.course_media_url }}" class="course-media__play-btn">
                    <svg
                      width="68"
                      height="68"
                      viewBox="0 0 68 68"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                    >
