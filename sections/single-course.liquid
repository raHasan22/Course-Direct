              <div class="certificate-img">
                {% if product.metafields.custom.certificate_image != blank %}
                  <img
                    width="100%"
                    height="455"
                    src="{{ product.metafields.custom.certificate_image | file_url }}"
                    alt="Certificate"
                  >
                {% else %}
                  <img width="100%" height="455" src="https://placehold.co/349x455" alt="Certificate">
                {% endif %}
              </div>
            </div>
          </div>
